<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.defianttech.diskdigger" platformBuildVersionCode="25" platformBuildVersionName="7.1.1">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_SUPERUSER"/>
    <application android:allowBackup="false" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:name="com.defianttech.diskdiggerpro.DiskDiggerApplication" android:supportsRtl="true">
        <activity android:configChanges="orientation|screenSize" android:name="com.defianttech.diskdiggerpro.DiskDiggerActivity" android:theme="@style/NoTitle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.defianttech.diskdiggerpro.DigDeeperActivity" android:theme="@style/NoTitle"/>
        <provider android:authorities="com.defianttech.diskdigger" android:grantUriPermissions="true" android:name="com.defianttech.diskdiggerpro.DiskDiggerContentProvider"/>
    </application>
</manifest>
