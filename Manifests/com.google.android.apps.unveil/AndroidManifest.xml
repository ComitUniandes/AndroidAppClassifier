<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="auto" package="com.google.android.apps.unveil">
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-feature android:name="android.hardware.location" android:required="false"/>
    <uses-feature android:name="android.hardware.location.gps" android:required="false"/>
    <uses-feature android:name="android.hardware.location.network" android:required="false"/>
    <uses-feature android:name="android.hardware.camera" android:required="false"/>
    <uses-feature android:name="android.hardware.camera.autofocus" android:required="false"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name_icon" android:name="com.google.android.apps.unveil.UnveilApplication">
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.AboutActivity" android:theme="@style/Theme.Sherlock"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.WhatsNewActivity" android:theme="@style/Theme.Sherlock"/>
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/app_name_icon" android:launchMode="singleTop" android:name="com.google.android.apps.unveil.SearchHistoryActivity" android:screenOrientation="landscape" android:theme="@style/SherlockFullscreen"/>
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/app_name_icon" android:launchMode="singleTop" android:name="com.google.android.apps.unveil.CaptureActivity" android:screenOrientation="landscape" android:theme="@style/SherlockFullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.android.apps.unveil.ACTION_TAKE_PHOTO"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.zxing.client.android.SCAN"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:host="zxing.appspot.com" android:path="/scan" android:scheme="http"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:host="www.google.com" android:path="/m/products/scan" android:scheme="http"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:host="www.google.co.uk" android:path="/m/products/scan" android:scheme="http"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:launchMode="singleTop" android:name="com.google.android.apps.unveil.ContinuousActivity" android:screenOrientation="portrait" android:theme="@style/actionBarOverlay"/>
        <activity android:configChanges="keyboardHidden|orientation" android:icon="@drawable/target" android:label="Tracking Test" android:launchMode="singleTop" android:name="com.google.android.apps.unveil.TrackingTestActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:icon="@drawable/target" android:label="Tracking Test Launcher" android:launchMode="singleTop" android:name="com.google.android.apps.unveil.TrackingSequenceActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/results_label" android:launchMode="singleTop" android:name="com.google.android.apps.unveil.ResultsActivity" android:theme="@style/actionBarOverlay" android:windowSoftInputMode="adjustResize"/>
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/results_label" android:name="com.google.android.apps.unveil.EpicFailActivity" android:theme="@style/actionBarOverlay" android:windowSoftInputMode="adjustResize"/>
        <activity android:name="com.google.android.apps.unveil.PuggleResultsActivity" android:theme="@style/Theme.Sherlock.Light"/>
        <activity android:configChanges="keyboardHidden|orientation" android:icon="@drawable/icon" android:label="@string/sent_to_goggles" android:name="com.google.android.apps.unveil.SubmittedResultsActivity" android:theme="@style/Theme.Sherlock"/>
        <activity android:configChanges="keyboardHidden|orientation" android:icon="@drawable/icon" android:label="@string/app_name_icon" android:name="com.google.android.apps.unveil.HistoryActivity" android:theme="@style/actionBarBlack">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH"/>
            </intent-filter>
            <meta-data android:name="android.app.searchable" android:resource="@xml/searchable"/>
            <meta-data android:name="android.app.default_searchable" android:value="com.google.android.apps.unveil.SearchHistoryActivity"/>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.MapHistoryActivity" android:theme="@style/Theme.Sherlock">
            <meta-data android:name="android.app.default_searchable" android:value="com.google.android.apps.unveil.SearchHistoryActivity"/>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.ContinuousFirstRunActivity" android:theme="@style/Theme.Sherlock.Light"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.TagImageActivity" android:theme="@style/Theme.Sherlock"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.ShareActivity" android:theme="@style/Theme.Sherlock">
            <intent-filter>
                <action android:name="android.intent.action.SEND"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="image/jpeg"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/welcome_to_goggles" android:name="com.google.android.apps.unveil.WelcomeActivity" android:theme="@style/Theme.Sherlock.Light"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.TutorialActivity" android:theme="@style/Theme.Sherlock"/>
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/tips_title" android:name="com.google.android.apps.unveil.TipsActivity" android:theme="@style/Theme.Sherlock.Light"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.RunQueryActivity" android:theme="@style/SherlockFullscreen"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.HistoryPreferenceActivity" android:theme="@style/Theme.Sherlock"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.TagActivity" android:theme="@style/Theme.Sherlock" android:windowSoftInputMode="adjustResize|stateVisible"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.NoteActivity" android:theme="@style/Theme.Sherlock" android:windowSoftInputMode="adjustResize|stateVisible"/>
        <activity android:label="@string/goggles_settings_screen_title" android:name="com.google.android.apps.unveil.PreferencesActivity" android:theme="@style/Theme.Sherlock"/>
        <activity android:label="@string/background_goggle_detailed_settings_title" android:name="com.google.android.apps.unveil.service.BackgroundGogglesPreferencesActivity" android:theme="@style/Theme.Sherlock"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.AttributionActivity" android:theme="@style/Theme.Sherlock"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.ExpandedResultsActivity" android:theme="@style/Theme.Sherlock.Light"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.google.android.apps.unveil.ReportAbuseActivity" android:theme="@style/Theme.Sherlock.Light" android:windowSoftInputMode="stateHidden"/>
        <activity android:label="@string/search_from_camera_title" android:name="com.google.android.apps.unveil.SfCFirstRunActivity" android:theme="@style/Theme.Sherlock.Light"/>
        <activity android:label="@string/search_history_title" android:name="com.google.android.apps.unveil.SHFirstRunActivity" android:theme="@style/Theme.Sherlock.Light"/>
        <activity android:label="@string/location_warning_title" android:name="com.google.android.apps.unveil.LocationFirstRunActivity" android:theme="@style/Theme.Sherlock.Light"/>
        <activity android:name="com.google.userfeedback.android.api.UserFeedbackActivity"/>
        <activity android:name="com.google.userfeedback.android.api.PreviewActivity"/>
        <activity android:excludeFromRecents="true" android:name="com.google.userfeedback.android.api.ShowTextActivity"/>
        <activity android:excludeFromRecents="true" android:name="com.google.userfeedback.android.api.ShowStringListActivity"/>
        <service android:name="com.google.userfeedback.android.api.SendUserFeedbackService"/>
        <receiver android:enabled="false" android:name="com.google.userfeedback.android.api.FeedbackConnectivityReceiver">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE"/>
            </intent-filter>
        </receiver>
        <provider android:authorities="@string/unveil_content_provider_authority" android:name="com.google.android.apps.unveil.env.UnveilContentProvider"/>
        <provider android:authorities="@string/suggestion_provider_authority" android:name="com.google.android.apps.unveil.history.SuggestionProvider"/>
        <receiver android:name="com.google.android.apps.unveil.ActivationReceiver">
            <intent-filter>
                <action android:name="com.google.android.apps.unveil.ACTIVATE"/>
            </intent-filter>
        </receiver>
        <service android:exported="false" android:name="com.google.android.apps.unveil.service.PictureRequestService">
            <intent-filter>
                <action android:name="com.google.android.apps.unveil.service.ENABLE_REQUEST"/>
                <action android:name="com.google.android.apps.unveil.service.POLL_REQUEST"/>
                <action android:name="com.google.android.apps.unveil.service.ON_SEEN_REQUEST"/>
                <action android:name="com.google.android.apps.unveil.service.DISABLE_REQUEST"/>
            </intent-filter>
        </service>
        <receiver android:exported="true" android:name="com.google.android.apps.unveil.service.LaunchReceiver">
            <intent-filter>
                <action android:name="com.android.camera.NEW_PICTURE"/>
                <data android:mimeType="image/*"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE"/>
            </intent-filter>
        </receiver>
        <uses-library android:name="com.google.android.maps"/>
    </application>
</manifest>
